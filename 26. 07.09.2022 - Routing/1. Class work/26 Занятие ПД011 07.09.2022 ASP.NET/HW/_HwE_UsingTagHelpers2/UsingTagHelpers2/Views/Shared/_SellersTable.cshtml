@model IEnumerable<UsingTagHelpers2.Models.Seller>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Ид</th>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Отчество</th>
        <th>Комиссионные,(%)</th>
        @* столбец для элементов управления *@
        <th>

        </th>
    </tr>
    </thead>

    <tbody>
    @foreach (var seller in Model)
    {
        @* применение сокращающего тег-хелпера *@
        <seller-table-row seller="seller"/>
    }
    </tbody>
</table>
