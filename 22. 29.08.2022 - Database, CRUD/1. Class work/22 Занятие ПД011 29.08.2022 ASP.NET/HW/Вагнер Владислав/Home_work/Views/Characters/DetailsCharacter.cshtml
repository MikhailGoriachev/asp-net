@model string
@using Home_work.Components.Characters
@{
    ViewBag.title = "Информация о персонаже";
    ViewBag.PageName = "Characters";
}
@section Head {
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css" />

}

<p>
    <a asp-controller="Characters" asp-action="Default">
        <i class="bi bi-arrow-left" style="font-size: 2rem"></i>
    </a>

</p>

<div>
    @await Component.InvokeAsync(nameof(SingleCharacterComponent),new{ url = Model});
</div>