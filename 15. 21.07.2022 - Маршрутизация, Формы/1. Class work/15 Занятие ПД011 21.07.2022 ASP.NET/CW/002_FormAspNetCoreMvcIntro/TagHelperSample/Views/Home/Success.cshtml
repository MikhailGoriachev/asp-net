@model TagHelperSample.Models.RegistrationBindingModel

<div class="col-5 mt-3 p-3 alert-success">
    <h3 class="text-black">Вы успешно зарегистрировались</h3>
</div>
<div class="col-5  mt-3 p-3">
    <ul class="list-group mb-5">
        <li class="list-group-item">Login: @Model.Login</li>
        <li class="list-group-item">Email: @Model.Email</li>
        <li class="list-group-item">TerAccepted: @Model.TermsAccepted</li>
    </ul>
    <a class="btn btn-success" asp-action="Index" asp-controller="Home">
        На главную
    </a>
</div>
